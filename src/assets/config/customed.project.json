{
  "color": ["#1e5ad9", "#a4c1ff", "#98ce59", "#ffce2f", "#f58167"],
  "backgroundColor": "#ffffff",
  "textStyle": {
    "fontFamily": "PingFangSC-Regular",
    "fontSize": "12",
    "lineHeight": "17"
  },
  "title": {
    "textStyle": {
      "color": "#323232"
    },
    "subtextStyle": {
      "color": "#6e7079"
    }
  },
  "line": {
    "itemStyle": {
      "borderWidth": 1
    },
    "lineStyle": {
      "width": 2
    },
    "symbolSize": 4,
    "symbol": "emptyCircle",
    "smooth": true
  },
  "radar": {
    "itemStyle": {
      "borderWidth": 1
    },
    "lineStyle": {
      "width": 2
    },
    "symbolSize": 4,
    "symbol": "emptyCircle",
    "smooth": true
  },
  "bar": {
    "itemStyle": {
      "barBorderWidth": 0,
      "barBorderColor": "#ccc"
    }
  },
  "pie": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "scatter": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "boxplot": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "parallel": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "sankey": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "funnel": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "gauge": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    }
  },
  "candlestick": {
    "itemStyle": {
      "color": "#eb5454",
      "color0": "#47b262",
      "borderColor": "#eb5454",
      "borderColor0": "#47b262",
      "borderWidth": 1
    }
  },
  "graph": {
    "itemStyle": {
      "borderWidth": 0,
      "borderColor": "#ccc"
    },
    "lineStyle": {
      "width": 1,
      "color": "#aaaaaa"
    },
    "symbolSize": 4,
    "symbol": "emptyCircle",
    "smooth": true,
    "color": ["#1e5ad9", "#a4c1ff", "#98ce59", "#ffce2f", "#f58167"],
    "label": {
      "color": "#404040"
    }
  },
  "categoryAxis": {
    "textStyle": {
      "color": "#404040"
    },
    "axisLine": {
      "show": false,
      "lineStyle": {
        "color": "#6E7079"
      }
    },
    "axisTick": {
      "show": false,
      "lineStyle": {
        "color": "#6E7079"
      }
    },
    "splitLine": {
      "show": false,
      "lineStyle": {
        "color": ["#E0E6F1"]
      }
    },
    "splitArea": {
      "show": false,
      "areaStyle": {
        "color": ["rgba(250,250,250,0.2)", "rgba(210,219,238,0.2)"]
      }
    }
  },
  "valueAxis": {
    "textStyle": {
      "color": "#404040"
    },
    "axisLine": {
      "show": false,
      "lineStyle": {
        "color": "#6E7079"
      }
    },
    "axisTick": {
      "show": false,
      "lineStyle": {
        "color": "#6E7079"
      }
    },
    "splitLine": {
      "show": true,
      "lineStyle": {
        "color": ["#e0e0e0"]
      }
    },
    "splitArea": {
      "show": false,
      "areaStyle": {
        "color": ["rgba(250,250,250,0.2)", "rgba(210,219,238,0.2)"]
      }
    }
  },
  "tooltip": {
    "axisPointer": {
      "lineStyle": {
        "color": "#cccccc",
        "width": 1
      },
      "crossStyle": {
        "color": "#cccccc",
        "width": 1
      }
    }
  },
  "visualMap": {
    "color": ["#1e5ad9", "#a4c1ff"]
  }
}
